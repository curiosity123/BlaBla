<Window x:Class="BlaBlaWpfClient.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlaBlaWpfClient"
        mc:Ignorable="d"
        Title="LoginWindow" Height="300" Width="300">
    <Window.DataContext>
        <local:LoginWindowViewModel></local:LoginWindowViewModel>
    </Window.DataContext>
    
    
    <Grid Margin="143,203,0,0">

        <TextBox Margin="-96,-79,96,101" Text="{Binding Login}" ></TextBox>
        <TextBox Margin="-96,-149,96,172" Text="{Binding Password}"></TextBox>


        <Button Margin="10,-10,24,10" Command="{Binding  OpenRegistrationCmd}">Go to registration</Button>
        <Button Margin="-119,-10,170,10" Command="{Binding TryLoginCmd}">Login</Button>
    </Grid>
</Window>
